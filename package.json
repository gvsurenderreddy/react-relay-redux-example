{
  "name": "wordpress-express",
  "version": "0.1.0",
  "description": "An example application using Node, Express, React, GraphQL and Relay with Wordpress",
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ramsaylanier/WordpressExpress"
  },
  "keywords": [
    "express",
    "webpack",
    "node",
    "react",
    "graphql",
    "relay",
    "wordpress"
  ],
  "author": "Ramsay Lanier",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ramsaylanier/WordpressExpress/issues"
  },
  "homepage": "https://github.com/ramsaylanier/WordpressExpress",
  "scripts": {
    "test": "",
    "start": "NODE_ENV=production node ./dist/server.js",
    "startdev": "NODE_ENV=dev babel-node --debug ./dev.js",
    "startdevprod": "NODE_ENV=production babel-node --debug ./server.js",
    "build-server": "babel -d ./dist ./server.js ./webpack.config.js",
    "build-schema": "npm run updateschema && babel -d ./dist/schema ./schema",
    "build-settings": "cp -R ./settings ./dist/ && babel -d ./dist/settings ./settings",
    "build": "rimraf dist && NODE_ENV=production webpack --config ./webpack.production.config.js --progress --profile --colors && npm run build-server && npm run build-schema",
    "deploy": "bash scripts/deploy.sh",
    "eslint": "eslint .",
    "jscs": "jscs .",
    "updateschema": "NODE_ENV=dev babel-node scripts/updateSchema",
    "introspection": "babel-node scripts/introspection.js"
  },
  "dependencies": {
    "babel-polyfill": "^6.3.14",
    "bootstrap": "^3.3.6",
    "dotenv": "^1.2.0",
    "express-http-proxy": "^0.6.0",
    "extract-text-webpack-plugin": "^0.8.2",
    "flag-icon-css": "^1.3.0",
    "graphql": "^0.4.14",
    "isomorphic-relay": "^0.4.2",
    "isomorphic-relay-router": "^0.5.1",
    "npm": "^3.5.3",
    "react": "^0.14.5",
    "react-css-modules": "^3.6.4",
    "react-dom": "^0.14.5",
    "react-helmet": "^2.3.1",
    "react-router": "^2.0.0-rc6",
    "react-router-relay": "^0.9.0",
    "react-scroll": "^1.0.15",
    "redux": "^3.3.1",
    "relay-local-schema": "^0.3.1",
    "wow.js": "^1.1.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.0.3",
    "babel": "^6.3.25",
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.26",
    "babel-eslint": "^5.0.0-beta6",
    "babel-loader": "^6.2.0",
    "babel-plugin-react-transform": "^2.0.0-beta1",
    "babel-plugin-transform-decorators-legacy": "1.x.x",
    "babel-plugin-transform-react-display-name": "^6.3.13",
    "babel-plugin-transform-runtime": "6.x.x",
    "babel-polyfill": "6.x.x",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.1",
    "babel-preset-stage-0": "^6.3.13",
    "babel-relay-plugin": "^0.7.3",
    "babel-runtime": "^6.3.19",
    "chai": "^3.2.0",
    "css-loader": "^0.19.0",
    "eslint": "^1.5.0",
    "eslint-plugin-react": "^3.4.2",
    "exports-loader": "^0.6.3",
    "express": "^4.13.3",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^1.6.1",
    "imports-loader": "^0.6.5",
    "jscs": "^2.1.1",
    "json-loader": "^0.5.3",
    "mocha": "^2.3.3",
    "mocha-jsdom": "^1.0.0",
    "node-sass": "^3.4.2",
    "null-loader": "^0.1.1",
    "postcss-loader": "^0.6.0",
    "react-addons-test-utils": "^0.14.3",
    "react-bootstrap": "^0.28.3",
    "react-relay": "^0.7.3",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.0",
    "redbox-react": "^1.2.0",
    "request": "^2.71.0",
    "rimraf": "^2.4.3",
    "sass-loader": "^3.1.2",
    "sinon": "^1.16.1",
    "sinon-chai": "^2.8.0",
    "stats-webpack-plugin": "^0.2.1",
    "style-loader": "^0.13.0",
    "sync-request": "^3.0.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-dev-server": "^1.14.0",
    "webpack-hot-middleware": "^2.2.0",
    "write-file-webpack-plugin": "^2.1.2"
  }
}
